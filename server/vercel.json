{
    "version": 2,
    "builds": [
      {
        "src": "./src/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "./client/dist",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "./src/app.js"
      }
    ]
  }